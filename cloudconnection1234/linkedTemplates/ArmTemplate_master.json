{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cloudconnection1234"
		},
		"LS_Databricksdev_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_Databricksdev'"
		},
		"LS_cloud178_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_cloud178'"
		},
		"LS_devstoragekpigen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_devstoragekpigen2'"
		},
		"clientsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'clientsql'"
		},
		"myownsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'myownsql'"
		},
		"source_cloud1289_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'source_cloud1289'"
		},
		"LS_Databricksdev_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0618-073838-m05xh0xf"
		},
		"LS_devstoragekpigen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devstoragekpigen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cloudconnection1234_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Databricksdev_accessToken": {
						"value": "[parameters('LS_Databricksdev_accessToken')]"
					},
					"LS_cloud178_connectionString": {
						"value": "[parameters('LS_cloud178_connectionString')]"
					},
					"LS_devstoragekpigen2_accountKey": {
						"value": "[parameters('LS_devstoragekpigen2_accountKey')]"
					},
					"clientsql_connectionString": {
						"value": "[parameters('clientsql_connectionString')]"
					},
					"myownsql_connectionString": {
						"value": "[parameters('myownsql_connectionString')]"
					},
					"source_cloud1289_connectionString": {
						"value": "[parameters('source_cloud1289_connectionString')]"
					},
					"LS_Databricksdev_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_Databricksdev_properties_typeProperties_existingClusterId')]"
					},
					"LS_devstoragekpigen2_properties_typeProperties_url": {
						"value": "[parameters('LS_devstoragekpigen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "cloudconnection1234_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'cloudconnection1234_ArmTemplate_0')]"
			]
		}
	]
}